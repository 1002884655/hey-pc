#app {
  .UserBg {
    width: 100%;
    position: relative;
    overflow: hidden;
    padding-bottom: 13.5%;
    background: #eee;
    z-index: 1;
    margin-top: 68px;
  }
  .MainContent {
    width: 70%;
    min-width: 900px;
    margin: 50px auto 0;
    position: relative;
    overflow: visible;
    z-index: 2;
    > div {
      position: relative;
      overflow: visible;
      &.Left {
        width: 21%;
        min-width: 250px;
      }
      &.flex-item {
        margin: 0 20px;
      }
      &.Right {
        width: 21%;
        min-width: 250px;
      }
    }
  }
}
