#app {
  background: rgba(0, 0, 0, 1);
  .IndexBannerContainer {
    width: 100%;
    padding-bottom: 56.25%;
    background: #333;
    position: relative;
    overflow: hidden;
    > div {
      width: 100%;
      position: absolute;
      left: 0;
      top: 0;
      bottom: 0;
      overflow: hidden;
    }
  }
  .ColumnList {
    width: 100%;
    position: relative;
    overflow: hidden;
    padding-bottom: 60px;
  }
}
